generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Message {
  id String @id @default(cuid())
  content String
  isLLM Boolean 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model UserPreference {
  id String @id @default(cuid())
  foodInventory String
  kitchenInventory String
  
}